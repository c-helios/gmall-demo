d - this.continuous - 1\n                } else if (end < this.continuous) {\n                    end = this.continuous\n                }\n                if (start < 1) {\n                    start = 1\n                }\n            }\n            // console.log(start,end,totalPage);\n            let pageNumberList = []\n            for (let index = start; index <= end; index++) {\n                pageNumberList.push(index)         \n            }\n\n            return {\n                start:start,\n                end:end,\n                totalPage:totalPage,\n                pageNumberList:pageNumberList,\n            }\n        }\n    },\n    methods: {\n        prePage() {\n            if (this.pageNo > 1) {\n                this.$emit('pageSearch',this.pageNo - 1);\n            }\n        },\n        nextPage() {\n            if (this.pageNo < this.page.totalPage) {\n                this.$emit('pageSearch',this.pageNo + 1);\n            }\n        },\n        changePage(pageNumber) {\n            if (pageNumber <= this.page.totalPage) {\n                this.$emit('pageSearch', pageNumber);\n            }\n            \n        }\n    }, \n}\n</script>\n\n<style lang=\"less\" scoped>\n.pagination {\n    text-align: center;\n    button {\n        margin: 0 5px;\n        background-color: #f4f4f5;\n        color: #606266;\n        outline: none;\n        border-radius: 2px;\n        padding: 0 4px;\n        vertical-align: top;\n        display: inline-block;\n        font-size: 13px;\n        min-width: 35.5px;\n        height: 28px;\n        line-height: 28px;\n        cursor: pointer;\n        box-sizing: border-box;\n        text-align: center;\n        border: 0;\n\n        &[disabled] {\n            color: #c0c4cc;\n            cursor: not-allowed;\n        }\n\n        &.active {\n            cursor: not-allowed;\n            background-color: #409eff;\n            color: #fff;\n        }\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}